 from tkinter import *        
win = Tk()  # win is a top or parent window 
win.geometry("200x100")   
b = Button(win, text = "Submit")   
b.pack()  #using pack() geometry 
win.mainloop()
 from tkinter import *  
root=Tk() 
label1 = Label(root, text="hi, welcome to GUI using Tkinter") 
label1.pack()  
root.mainloop()
 from tkinter import * 
root =Tk() 
c = Checkbutton(root, text = "Python") 
c.pack()  
c1 = Checkbutton(root, text = "C++") 
c1.pack() 
c2 = Checkbutton(root, text = "C") 
c2.pack() 
root.geometry("400x400") 
root.mainloop()
